{
  "name": "docker-playground",
  "version": "0.0.32",
  "private": true,
  "dependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.3",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.4",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "source-map-explorer": "1.5.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "analyze": "source-map-explorer --html build/static/js/main.* > /tmp/treemap.html",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "format": "prettier-eslint --write src/**/*.{js,json,jsx,scss}",
    "lint:script": "eslint src/**/*.js",
    "lint:script:fix": "eslint --fix src/**/*.js",
    "lint:stylesheet": "stylelint \"src/**/*.scss\"",
    "test": "node scripts/test.js --config jest.config.json",
    "test:coverage": "yarn test --coverage --no-cache --silent",
    "test:coverage:ci": "cat /usr/src/app/coverage/lcov.info | /usr/src/app/node node_modules/.bin/coveralls",
    "test:coverage:ci:legacy": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "test:coverage:ci:travis": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "test:verbose": "node scripts/test.js --env=jsdom --verbose --config jest.config.json",
    "test:staged": "cross-env CI=true node scripts/test.js --config jest.config.json",
    "test:silent": "node scripts/test.js --env=jsdom --silent --config jest.config.json",
    "type": "flow",
    "type:check": "flow check",
    "type:check:focus": "flow focus-check",
    "type:libdef": "flow-typed install",
    "start:https": "HTTPS=true yarn start"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "cross-env": "5.2.0",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "prettier": "1.14.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "replace": "^1.0.0",
    "stylelint": "^9.6.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.2"
  }
}
